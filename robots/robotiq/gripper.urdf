<?xml version="1.0" ?>
<!-- ======================================================================= -->
<!-- |                           ROBOTIQ GRIPPER                           | -->
<!-- ======================================================================= -->
<robot name="gripper">
  <!-- ============ gripper ============= -->
  <link name="tool">
    <inertial>
      <mass value="0.5"/> 
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.001094" iyy="0.001119" izz="0.001093" ixy="0.000007" ixz="0.000001"  iyz="-0.000002" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="graphics/toolGripperRobotiq.obj"/>
      </geometry>      
    </visual> 
    <collision>
       <origin xyz="0 0 0" rpy="0 0 0"/>
       <geometry>
         <mesh filename="graphics/toolGripperRobotiq.obj"/>
       </geometry>
    </collision>    
  </link>
</robot>



